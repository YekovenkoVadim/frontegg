  "private": true,
  "author": {
    "name": "Frontegg",
    "email": "hello@frontegg.com",
    "url": "https://frontegg.com"
  },
  "license": "MIT",
  "dependencies": {
    "@estruyf/github-actions-reporter": "^1.3.0",
    "@frontegg/react": "^6.0.18",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "playwright": "^1.40.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^5.3.4",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "docker-build": "docker build -t my-react-app .",
    "docker-run": "docker run --rm -p 3000:3000 -v $(pwd):/app my-react-app",
    "build-docker-test": "docker build -t playwright-tests .",
    "run-docker-test": "USERNAME=vadim.yekovenko@gmail.com PASSWORD=Qwerty1! docker run playwright-tests",
    "start": "react-scripts start",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "pw-test-run": "VIDEO=1 USERNAME=vadim.yekovenko@gmail.com PASSWORD=Qwerty1! npx playwright test --headed",
    "pw-test-ui-mode": "USERNAME=vadim.yekovenko@gmail.com PASSWORD=Qwerty1! playwright test --ui",
    "e2e": "VIDEO=1 USERNAME=vadim.yekovenko@gmail.com PASSWORD=Qwerty1! npx playwright test --grep @e2e --config ./playwright.config.ts --headed && playwright show-report",
    "e2e-parallel": "VIDEO=1 USERNAME=vadim.yekovenko@gmail.com PASSWORD=Qwerty1! npx playwright test --grep @e2e --config ./playwright.config.ts --headed --workers 3 && playwright show-report",
    "pw-test-allure-run": "ALLURE_RESULTS_DIR=allure-results USERNAME=vadim.yekovenko@gmail.com PASSWORD=Qwerty1! npx playwright test --grep @e2e --config ./playwright.config.ts --headed --workers 3 --reporter=line,allure-playwright",
    "allure-generate": "allure generate ./allure-results -o allure-report --clean && allure open ./allure-report",
    "e2e-ci": "VIDEO=1 npx playwright test --grep @e2e --config ./playwright.config.ts || :",
    "e2e-parallel-ci": "VIDEO=1 npx playwright test --grep @e2e --config ./playwright.config.ts --workers 3 || :",
    "allure:generate": "npx allure generate ./allure-results --clean",
    "allure:open": "npx allure open ./allure-report",
    "posttest": "npm run allure:generate"
   },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@playwright/test": "^1.40.1",
    "@types/node": "^20.10.6",
    "allure-commandline": "^2.25.0",
    "allure-playwright": "^2.10.0",
    "gh-pages": "^6.1.1",
    "typescript": "^4.9.5"
  }
}
